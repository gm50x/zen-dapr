{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/sandbox/src",
  "projects": {
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "sandbox": {
      "type": "application",
      "root": "apps/sandbox",
      "entryFile": "main",
      "sourceRoot": "apps/sandbox/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sandbox/tsconfig.app.json"
      }
    },
    "dapr": {
      "type": "library",
      "root": "libs/dapr",
      "entryFile": "index",
      "sourceRoot": "libs/dapr/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dapr/tsconfig.lib.json"
      }
    },
    "secrets-provider": {
      "type": "library",
      "root": "libs/secrets-provider",
      "entryFile": "index",
      "sourceRoot": "libs/secrets-provider/src",
      "compilerOptions": {
        "tsConfigPath": "libs/secrets-provider/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "publisher": {
      "type": "library",
      "root": "libs/publisher",
      "entryFile": "index",
      "sourceRoot": "libs/publisher/src",
      "compilerOptions": {
        "tsConfigPath": "libs/publisher/tsconfig.lib.json"
      }
    },
    "state-provider": {
      "type": "library",
      "root": "libs/state-provider",
      "entryFile": "index",
      "sourceRoot": "libs/state-provider/src",
      "compilerOptions": {
        "tsConfigPath": "libs/state-provider/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/sandbox/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/sandbox"
}